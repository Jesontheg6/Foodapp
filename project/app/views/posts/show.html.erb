<br>
<div class="container" align="center">
<div class="posts-wrapper row">
  <% @post.each do |post| %>
  <% post.photos.each do |o| %>
    <div class="post">
      <div class="post-head">
      	 <div class="name", , style="float: left">
      <%= link_to post.user.name ,show_user_profile_path(post) %>
        <br>
        <div class= "location", style="float: left">
         	<%= link_to "view location", show_location_path(post) %>
         </div>
        </div>
           <div class="post_time", style="float: right">
          <%= post.created_at.localtime.strftime '%Y-%m-%d'%>
        </div>
      </div>
      <div class="image center-block">
        <%= link_to (image_tag o.url, class:'img-responsive'), post_path(post) %>
      </div>
      <p class="caption", align="left">
        <%= post.review %>
        <br>
        Rating:
      <%= post.ratings %>
    </p>
    </div>
  <% end %>
  <% end %>
</div>
</div>

